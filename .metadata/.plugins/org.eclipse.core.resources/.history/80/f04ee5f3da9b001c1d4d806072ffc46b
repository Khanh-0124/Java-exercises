package MyPack;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		try (Scanner sc = new Scanner(System.in)) {
			System.out.println("Nhập lựa chọn");
			int chon;
			chon = sc.nextInt();
			while (chon >= 0) {

				switch (chon) {
				case 1:
					break;
				case 2:
					break;
				case 3: 
					ChuongTrinhTinh.GiaiThua();
					break;
				}
			}
		}
	}
}
