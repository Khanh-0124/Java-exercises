package MyPack;

import javax.swing.JFrame;

public class ViDu {

	public static void main(String[] args) {
		JFrame jf = new JFrame();
		jf.setVisible(true);
		jf.setSize(600, 400);
		jf.setTitle("Java swing");
		
		while(true) {
			System.out.println("hello");
		}
		jf.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}

}
