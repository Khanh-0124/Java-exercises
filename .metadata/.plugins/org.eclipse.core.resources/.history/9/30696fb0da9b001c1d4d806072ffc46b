package MyPack;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		ChuongTrinhTinh ct = new ChuongTrinhTinh();
		System.out.println("Nhập lựa chọn");
		int chon;
		chon = sc.nextInt();
		while (chon >= 0) {

			switch (chon) {
			case 1:
				break;
			case 2:
				break;
			}
		}
		ct.GiaiThua();
	}
}
